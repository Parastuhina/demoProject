{
  "name": "figma-html-export",
  "version": "1.0.0",
  "private": true,
  "description": "Generate static HTML pages from a Figma file (frames rendered as images with page indexes).",
  "main": "scripts/generate-from-figma.js",
  "scripts": {
    "generate": "node scripts/generate-from-figma.js",
    "clean": "rm -rf dist",
    "preview": "http-server dist -p ${PORT:-8080} -a 0.0.0.0 -c-1",
    "start": "npm run generate && npm run preview"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

